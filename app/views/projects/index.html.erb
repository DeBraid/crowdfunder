<h1>Projects: View ALL (Index)</h1>
<p>Listing projects</p>


<% @projects.each do |project| %>
<p>
  <%= project[:title] %><br />
  <%= project[:teaser] %> <br />
  <%= project[:description] %> <br />
  <%= project[:goal] %> <br />
  <%= link_to "#{project[:title]}", "/projects/#{project.id}" %><li class="title"></li>
  </p>

<% end %>